@model List<Post>
<div class="card">
                <div class="card-header">
                    <h5 class="card-title">New Post</h5>
                </div>
                <div class="card-body">
                    @foreach(var post in Model){
                        <div class="mb-3">
                            <a href="/posts/@post.Url"></a>
                            <h6 class="mb-0">@post.Title</h6>
                        </div>
                    }
                </div>
            </div>